{
    "name": "andrey-helldar/spammers-server",
    "description": "A spammers server package",
    "type": "library",
    "license": "MIT",
    "keywords": ["spam", "spammer", "laravel", "black list"],
    "support": {
        "issues": "https://github.com/andrey-helldar/spammers-server/issues",
        "source": "https://github.com/andrey-helldar/spammers-server"
    },
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/database": "^5.5|^6.0",
        "illuminate/support": "^5.5|^6.0",
        "nesbot/carbon": "^1.0|^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "suggest": {
        "symfony/thanks": "Give thanks (in the form of a GitHub) to your fellow PHP package maintainers"
    },
    "autoload": {
        "psr-4": {
            "Helldar\\SpammersServer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Helldar\\SpammersServer\\ServiceProvider"
            ]
        }
    }
}
